    "@types/react-dom": "^16.9.6",
    "@types/fetch-mock": "^7.3.3",
    "chai": "^4.2.0",
    "common-js": "^0.3.8",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^4.5.0",
    "mocha": "^8.2.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
